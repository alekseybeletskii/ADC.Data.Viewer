<AnchorPane fx:id="plotsLayout" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" styleClass="AnchorPane" stylesheets="/css/plotter.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adc.data.viewer.ui.PlotterController">
   <children>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" mouseTransparent="true" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="legend" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" styleClass="signalLabel">
               <font>
                  <Font size="10.0" />
               </font>
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="signalIndexLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" styleClass="signalIndexLabel" textAlignment="RIGHT" textFill="#1e68b0">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <left>
            <HBox spacing="2.0">
               <children>
                  <Button fx:id="Settings" mnemonicParsing="false" onAction="#handlePlotterSettings" text="Settings">
                     <font>
                        <Font size="10.0" />
                     </font>
                  </Button>
                  <ToggleButton fx:id="SubtractSignal" mnemonicParsing="false" onAction="#handleSubtractSignal" text="- Signal">
                     <font>
                        <Font size="10.0" />
                     </font>
                  </ToggleButton>
                    <ToggleButton fx:id="Raw" onAction="#handleRaw" text="Raw">
      
                           <font>
                        <Font size="10.0" />
                     </font>
                    </ToggleButton>
                    <ToggleButton fx:id="RawAndSGFilter" onAction="#handleRawAndSGFilter" text="Raw&amp;Filter">
      
                           <font>
                        <Font size="10.0" />
                     </font>
                    </ToggleButton>
                    <ToggleButton fx:id="SGFilter" onAction="#handleSGFilter" text="Filter">
      >
                           <font>
                        <Font size="10.0" />
                     </font>
                    </ToggleButton>
                  <ToggleButton fx:id="subtractSGFilter" onAction="#handleSGFiltered" text="Raw-Filter">
                     <font>
                        <Font size="10.0" />
                     </font>
                  </ToggleButton>
                  <Label minHeight="0.0" minWidth="0.0" text="x=......; y=......" fx:id="xyLabel">
                     <font>
                        <Font name="System Italic" size="12.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="70.0" />
               </padding>
               <BorderPane.margin>
                  <Insets bottom="2.0" top="2.0" />
               </BorderPane.margin>
            </HBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
